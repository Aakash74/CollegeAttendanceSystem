        android:supportsRtl="true"
        android:theme="@style/Theme.AttendenceSystem">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.attendencesystem.activity.LoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.attendencesystem.activity.MenuActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.attendencesystem.activity.AddFacultyActivity"
