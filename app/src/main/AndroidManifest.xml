        </activity>
        <activity
            android:name="com.example.attendencesystem.activity.AddAttendanceActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.attendencesystem.activity.ViewAttendanceByFacultyActivity"
            android:label="@string/app_name" >
